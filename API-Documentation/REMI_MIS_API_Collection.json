						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"email\": \"newuser@example.com\",\n    \"password\": \"SecurePass@123\",\n    \"password_confirm\": \"SecurePass@123\",\n    \"first_name\": \"John\",\n    \"last_name\": \"Doe\",\n    \"employee_id\": \"EMP001\",\n    \"role\": \"role-uuid-here\",\n    \"profile\": {\n        \"office_level\": \"DISTRICT\",\n        \"province\": \"province-uuid\",\n        \"district\": \"district-uuid\",\n        \"phone_number\": \"9841234567\"\n    }\n}"
						},
						"url": {
							"raw": "{{base_url}}/auth/register/",
							"host": ["{{base_url}}"],
